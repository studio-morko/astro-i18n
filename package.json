{
  "name": "@mannisto/astro-i18n",
  "version": "0.0.1",
  "description": "Astro client-side internationalization (i18n) package for static sites",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./LocaleRedirect.astro": "./dist/components/LocaleRedirect.astro"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node scripts/build-components.js",
    "dev": "tsup --watch",
    "test": "vitest",
    "format": "biome format . --write",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "astro",
    "i18n",
    "internationalization",
    "localization",
    "locale"
  ],
  "author": "Ere Männistö",
  "license": "MIT"
}
